<%= form_for(@patch_note) do |f| %>

  <div class="form-field">
    <div class="form-field-outer-wrapper">
      <div class="form-field-wrapper">
        <div class="form-field-label">Version</div>
          <%= f.text_field :version, class: "text-field" %>
      </div>
    </div>
  </div>

  <div class="form-field">
    <div class="form-field-outer-wrapper">
      <div class="form-field-wrapper">
        <div class="form-field-label">Notes</div>
        <%= f.text_area :description, class: "text-field" %>
      </div>
    </div>
  </div>

  <div class="form-field">
    <div class="form-field-outer-wrapper">
      <div class="form-field-wrapper">
        <%= f.submit "Submit", class: "button button-green" %>
        <%= link_to 'Back', about_path, class: "button button-red" %>
      </div>
    </div>
  </div>

<% end %>
