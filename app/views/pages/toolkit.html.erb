<script>
function scrollTo(elementClass){
  var $container = $("html,body");
  var $scrollTo = $("."+elementClass);

  $container.animate({scrollTop: $scrollTo.offset().top - $container.offset().top + $container.scrollTop(), scrollLeft: 0},1000);
}
</script>