<div class="song-outer-wrapper">
  <div class="song-wrapper">
    <iframe width="420" height="315" src="https://www.youtube.com/embed/<%= song.strip_url %>?showinfo=0" class="song"></iframe>
    <div class="song-links-wrapper">
      <div class="song-name"><%= song.name %></div>
      <%= link_to "Edit", edit_song_path(song), class: "link-yellow" %>
      <%= link_to 'Destroy', song,  class: "link-red", method: :delete, data: { confirm: 'Are you sure?' } %>
    </div>
  </div>
</div>